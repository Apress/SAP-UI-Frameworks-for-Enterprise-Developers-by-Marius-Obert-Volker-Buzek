        "technology": "UI5",
        "icons": {
            "icon": "sap-icon://search"
        }
    },
    "sap.ui5": {
        "dependencies": {
            "minUI5Version": "1.106",
            "libs": {
                "sap.m": {},
                "sap.ui.integration": {}
            },
            "components": {
                "sap.ushell.components.cepsearchresult.app": {
                    "minVersion": "1.100.0",
                    "lazy": false
                }
            }
        },
        "rootView": {
            "viewName": "sap.ushell.components.cepsearchresult.app.cards.searchresultwidget.Main",
            "type": "XML",
            "async": true,
            "id": "card"
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": [
                {
                    "uri": "./css/style.css"
                }
            ]
        }
    },
    "sap.card": {
        "type": "Component",
        "configuration": {
            "parameters": {
                "categories": {
                    "value": "all"
                },
                "searchTerm": {
                    "value": ""
                },
                "edition": {
                    "value": "standard"
                },
                "currentView": {
                    "value": "default"
                }
            }
        },
        "header": {
            "title": "",
            "icon": {
                "src": "sap-icon://search"
            },
            "visible": false
        }
